{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "semi": 0,
    "import/prefer-default-export": 0,
    "arrow-parens": 0,
    "radix": 0,
    "implicit-arrow-linebreak": 0,
    "no-console": "off",
    "operator-linebreak": ["error", "after"]
  }
}
